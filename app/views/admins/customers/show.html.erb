<h3><span class="label label-default">新規会員登録</span></h1>


<table>
  <tr>
    <td>顧客ID</td>
    <td><%= @customer.id %></td>
  </tr>

  <tr>
    <td>氏名</td>
    <td><%= @customer.last_name + @customer.first_name %></td>
  </tr>

  <tr>
    <td>フリガナ</td>
    <td><%= @customer.last_name_kana + @customer.first_name_kana %></td>
  </tr>

  <tr>
    <td>郵便番号</td>
    <td><%= @customer.postal_code %></td>
  </tr>

  <tr>
    <td>住所</td>
    <td><%= @customer.address %></td>
  </tr>

  <tr>
    <td>電話番号</td>
    <td><%= @customer.telephone_number %></td>
  </tr>

  <tr>
    <td>メールアドレス</td>
    <td><%= @customer.email %></td>
  </tr>
</table>

<div>
  <%= link_to '編集する', edit_admins_customer_path, class: "btn btn-primary" %>
  <%#= link_to '注文履歴一覧を見る', %>
</div>